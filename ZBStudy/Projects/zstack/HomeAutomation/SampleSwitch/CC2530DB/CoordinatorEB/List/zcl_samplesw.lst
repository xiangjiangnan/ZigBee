    259           * @fn      zclSampleSw_ProcessZDOMsgs()
    260           *
    261           * @brief   Process response messages
    262           *
    263           * @param   none
    264           *
    265           * @return  none
    266           */
    267          void zclSampleSw_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    268          {
    269            switch ( inMsg->clusterID )
    270            {
    271              case End_Device_Bind_rsp:
    272                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    273                {
    274                  // Light LED
    275                  //HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    276                  osal_start_timerEx(zclSampleSw_TaskID, SAMPLESW_ONOFF_EVT, SAMPLESW_ONOFF_TIMEOUT); 
    277                }
    278            }
    279          }
