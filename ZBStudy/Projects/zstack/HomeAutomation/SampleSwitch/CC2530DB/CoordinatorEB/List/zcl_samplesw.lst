    281          /*********************************************************************
    282           * @fn      zclSampleSw_HandleKeys
    283           *
    284           * @brief   Handles all key events for this device.
    285           *
    286           * @param   shift - true if in shift/alt.
    287           * @param   keys - bit field for key events. Valid entries:
    288           *                 HAL_KEY_SW_4
    289           *                 HAL_KEY_SW_3
    290           *                 HAL_KEY_SW_2
    291           *                 HAL_KEY_SW_1
    292           *
    293           * @return  none
    294           */
    295          static void zclSampleSw_HandleKeys( byte shift, byte keys )
    296          {
    297            zAddrType_t dstAddr;
    298            (void)shift;  // Intentionally unreferenced parameter
    299          
    300            if ( keys & HAL_KEY_SW_2 )
    301            {
    302              //HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    303          
    304              // Initiate an End Device Bind Request, this bind request will
    305              // only use a cluster list that is important to binding.
    306              dstAddr.addrMode = afAddr16Bit;
    307              dstAddr.addr.shortAddr = 0;   // Coordinator makes the match
    308              ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    309                                     SAMPLESW_ENDPOINT,
    310                                     ZCL_HA_PROFILE_ID,
    311                                     0, NULL,   // No incoming clusters to bind
    312                                     zclSampleSw_SimpleDesc.AppNumOutClusters, 
    313                                     zclSampleSw_SimpleDesc.pAppOutClusterList,
    314                                     TRUE );
    315            }
    316          }
