    373                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    374                                      FALSE );
    375              }
    376          
    377              if ( keys & HAL_KEY_SW_3 )
    378              {
    379              }
    380          
    381              if ( keys & HAL_KEY_SW_4 )
    382              {
    383                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    384                // Initiate a Match Description Request (Service Discovery)
    385                dstAddr.addrMode = AddrBroadcast;
