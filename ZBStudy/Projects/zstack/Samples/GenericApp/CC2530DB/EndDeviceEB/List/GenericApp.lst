    328          static void GenericApp_HandleKeys( uint8 shift, uint8 keys )
    329          {
    330            zAddrType_t dstAddr;
    331          
    332            // Shift is used to make each button/switch dual purpose.
    333            if ( shift )
    334            {
    335              if ( keys & HAL_KEY_SW_1 )
    336              {
    337              }
    338              if ( keys & HAL_KEY_SW_2 )
    339              {
    340              }
    341              if ( keys & HAL_KEY_SW_3 )
    342              {
    343              }
    344              if ( keys & HAL_KEY_SW_4 )
    345              {
    346              }
    347            }
    348            else
    349            {
    350              if ( keys & HAL_KEY_SW_1 )
    351              {
    352                // Since SW1 isn't used for anything else in this application...
    353          #if defined( SWITCH1_BIND )
    354                // we can use SW1 to simulate SW2 for devices that only have one switch,
    355                keys |= HAL_KEY_SW_2;
    356          #elif defined( SWITCH1_MATCH )
    357                // or use SW1 to simulate SW4 for devices that only have one switch
    358                keys |= HAL_KEY_SW_4;
    359          #endif
    360              }
    361          
    362              if ( keys & HAL_KEY_SW_2 )
    363              {
    364                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    365          
    366                // Initiate an End Device Bind Request for the mandatory endpoint
    367                dstAddr.addrMode = Addr16Bit;
    368                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    369                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
