    264          /*********************************************************************
    265           * @fn      GenericApp_ProcessZDOMsgs()
    266           *
    267           * @brief   Process response messages
    268           *
    269           * @param   none
    270           *
    271           * @return  none
    272           */
    273          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    274          {
    275            switch ( inMsg->clusterID )
    276            {
    277              case End_Device_Bind_rsp:
    278                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    279                {
    280                  // Light LED
    281                  HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    282                }
    283          #if defined( BLINK_LEDS )
    284                else
    285                {
    286                  // Flash LED to show failure
    287                  HalLedSet ( HAL_LED_4, HAL_LED_MODE_FLASH );
    288                }
    289          #endif
    290                break;
    291          
    292              case Match_Desc_rsp:
    293                {
    294                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    295                  if ( pRsp )
    296                  {
    297                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    298                    {
    299                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    300                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    301                      // Take the first endpoint, Can be changed to search through endpoints
    302                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    303          
    304                      // Light LED
    305                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    306                    }
    307                    osal_mem_free( pRsp );
    308                  }
    309                }
    310                break;
    311            }
    312          }
