    288           */
    289          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    290          {
    291            switch ( inMsg->clusterID )
    292            {
    293              case End_Device_Bind_rsp:
    294                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    295                {
    296                    osal_start_timerEx( GenericApp_TaskID,
    297                                          GENERICAPP_SEND_BINDMSG_EVT,
    298                                          GENERICAPP_SEND_BINDMSG_TIMEOUT );
    299                }
    300                break;
    301          
    302              case Match_Desc_rsp:
    303                {
    304                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    305                  if ( pRsp )
    306                  {
    307                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    308                    {
    309                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    310                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    311                      // Take the first endpoint, Can be changed to search through endpoints
    312                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    313          
    314                      // Light LED
    315                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    316                      
    317                    }
    318                    osal_mem_free( pRsp );
    319                  }
    320                }
    321                break;
    322            }
    323          }
