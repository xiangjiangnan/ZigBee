    332          static void GenericApp_HandleKeys( uint8 shift, uint8 keys )
    333          {
    334            zAddrType_t dstAddr;
    335          
    336            // Shift is used to make each button/switch dual purpose.
    337            if ( shift )
    338            {
    339              if ( keys & HAL_KEY_SW_1 )
    340              {
    341              }
    342              if ( keys & HAL_KEY_SW_2 )
    343              {
    344              }
    345              if ( keys & HAL_KEY_SW_3 )
    346              {
    347              }
    348              if ( keys & HAL_KEY_SW_4 )
    349              {
    350              }
    351            }
    352            else
    353            {
    354              if ( keys & HAL_KEY_SW_1 )
    355              {
    356                // Since SW1 isn't used for anything else in this application...
    357          #if defined( SWITCH1_BIND )
    358                // we can use SW1 to simulate SW2 for devices that only have one switch,
    359                keys |= HAL_KEY_SW_2;
    360          #elif defined( SWITCH1_MATCH )
    361                // or use SW1 to simulate SW4 for devices that only have one switch
    362                keys |= HAL_KEY_SW_4;
    363          #endif
    364              }
    365          
    366              if ( keys & HAL_KEY_SW_2 )
    367              {
    368                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    369          
    370                // Initiate an End Device Bind Request for the mandatory endpoint
    371                dstAddr.addrMode = Addr16Bit;
    372                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    373                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    374                                      GenericApp_epDesc.endPoint,
