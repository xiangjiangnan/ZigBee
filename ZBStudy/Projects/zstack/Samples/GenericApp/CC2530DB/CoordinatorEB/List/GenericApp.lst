    282           * @fn      GenericApp_ProcessZDOMsgs()
    283           *
    284           * @brief   Process response messages
    285           *
    286           * @param   none
    287           *
    288           * @return  none
    289           */
    290          static void GenericApp_ProcessZDOMsgs( zdoIncomingMsg_t *inMsg )
    291          {
    292            switch ( inMsg->clusterID )
    293            {
    294              case End_Device_Bind_rsp:
    295                if ( ZDO_ParseBindRsp( inMsg ) == ZSuccess )
    296                {
    297                    osal_start_timerEx( GenericApp_TaskID,
    298                                          GENERICAPP_SEND_BINDMSG_EVT,
    299                                          GENERICAPP_SEND_BINDMSG_TIMEOUT );
    300                }
    301                break;
    302          
    303              case Match_Desc_rsp:
    304                {
    305                  ZDO_ActiveEndpointRsp_t *pRsp = ZDO_ParseEPListRsp( inMsg );
    306                  if ( pRsp )
    307                  {
    308                    if ( pRsp->status == ZSuccess && pRsp->cnt )
    309                    {
    310                      GenericApp_DstAddr.addrMode = (afAddrMode_t)Addr16Bit;
    311                      GenericApp_DstAddr.addr.shortAddr = pRsp->nwkAddr;
    312                      // Take the first endpoint, Can be changed to search through endpoints
    313                      GenericApp_DstAddr.endPoint = pRsp->epList[0];
    314          
    315                      // Light LED
    316                      HalLedSet( HAL_LED_4, HAL_LED_MODE_ON );
    317                      
    318                    }
    319                    osal_mem_free( pRsp );
    320                  }
    321                }
    322                break;
    323            }
    324          }
