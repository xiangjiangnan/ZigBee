    377                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)GenericApp_ClusterList,
    378                                      FALSE );
    379              }
    380          
    381              if ( keys & HAL_KEY_SW_3 )
    382              {
    383              }
    384          
    385              if ( keys & HAL_KEY_SW_4 )
    386              {
    387                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    388                // Initiate a Match Description Request (Service Discovery)
    389                dstAddr.addrMode = AddrBroadcast;
